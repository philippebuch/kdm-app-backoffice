.rvo-page-section
    .rvo-toolbar 
        .rvo-toolbar-nav
            ul
                li
                    a.rvo-current(href="#") Véhicules en cours de repasse (23)
                li
                    a(href="#") Historique
        .rvo-buttons
            button.rvo-button-bordered.rvo-disabled(type="button") Retour CarPlaza
            button.rvo-button-bordered.rvo-disabled(type="button") Déclarer vendu
include ../../shared/filters.pug
.rvo-page-section.rvo-margin-t15
    .rvo-table-scroll
        table.rvo-table.rvo-bordered.rvo-padding-m
            +table-header()
                each header in jsondata.venteProImmediate.headers
                    +table-header-cell(header.label, header.align, header.fittocell, header.checkbox, header.fontsize, header.colspan, header.sort, header.filter, header.stickycol, header.cellClasses, header.tooltip, header.tooltipLink)
            +table-body()
                each row, index in jsondata.venteProImmediate.rows
                    +table-body-row()
                        each cell in jsondata.venteProImmediate.rows[index].cells
                            +table-body-cell(cell.label, cell.align, cell.toggle, cell.menu, cell.menuposition, cell.menuicon, cell.status, cell.checkbox, cell.minibutton, cell.minibuttontitle, cell.notif, cell.fontsize, cell.colspan, cell.field, cell.alwaysShow, cell.radio, cell.validation, cell.labelClasses, cell.cellClasses, cell.stickycol, cell.islink)